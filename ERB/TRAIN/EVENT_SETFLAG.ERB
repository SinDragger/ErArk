		RETURN 0

	BASE:(ARG:1):气力 = MIN(BASE:(ARG:1):气力 + ARG, MAXBASE:(ARG:1):气力)
CASE "气力回復上限无视"
	SIF ARG:1 < 0
		RETURN 0
	SIF ARG:1 == 0 && TARGET > 0
		ARG:1 = TARGET
	BASE:(ARG:1):气力 += ARG
CASE "气力回復上限まで"
	SIF ARG:1 < 0
		RETURN 0
	SIF ARG:1 == 0 && TARGET > 0
		ARG:1 = TARGET

	BASE:(ARG:1):气力 = MIN(BASE:(ARG:1):气力 + ARG, MAXBASE:(ARG:1):气力)

CASE "精力回復"
	SIF ARG:1 < 0
