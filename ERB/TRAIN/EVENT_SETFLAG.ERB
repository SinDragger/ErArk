	DOWNBASE:气力 += (TALENT:V损伤 + TALENT:A损伤)*50
	DOWNBASE:体力 += (TALENT:V损伤 + TALENT:A损伤)*50

	;BASEからDOWNBASEを引く
	BASE:体力 = MAX(BASE:体力 - DOWNBASE:体力, 0)
	;一方的にむさぼったあとなら
	SIF TEQUIP:逆推 && BASE:体力 <= 0
		BASE:体力 = 1
	BASE:气力 = MAX(BASE:气力 - DOWNBASE:气力, 0)
