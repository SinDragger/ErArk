	IF STATE("倍化术", PLAYER) && TEQUIP:逆推 == 0
		SELECTCASE SELECTCOM
		CASE 3, 13, 90 TO 129, 139, IS >= 170
		CASEELSE
			TIMES DOWNBASE:体力, 2.00
			TIMES DOWNBASE:气力, 2.00
		ENDSELECT
	ENDIF

	;-------------------------------------------------
	;气力０、精力０による上下之处理
