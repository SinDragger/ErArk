	CASEELSE
		BASE:ARG:身长 = LOCAL
	ENDSELECT
	TALENT:ARG:高大 = 0
	TALENT:ARG:娇小 = 1
	TALENT:ARG:妖精 = 0
	CALL CALC, "BＷＨ计測", ARG
CASE "普通体型化"
	LOCAL = 145 + RAND:11 + RAND:5 + RAND:5
	SELECTCASE BASE:ARG:身长
	CASE IS <= NUM("娇小之上限")
		BASE:ARG:身长 = MAX(LOCAL, BASE:ARG:身长 + 10)
	CASE IS > NUM("普通之上限")
		BASE:ARG:身长 = MIN(LOCAL, BASE:ARG:身长 - 10)
	CASEELSE
		BASE:ARG:身长 = LOCAL
	ENDSELECT
	TALENT:ARG:高大 = 0
	TALENT:ARG:娇小 = 0
	CALL CALC, "BＷＨ计測", ARG
CASE "高大化"
	BASE:ARG:身长 = MAX(164 + RAND:9 + RAND:9, BASE:ARG:身长 + 10)
	TALENT:ARG:高大 = 1
	TALENT:ARG:娇小 = 0
	CALL CALC, "BＷＨ计測", ARG
CASE "BＷＨ计測"
	VARSET LOCAL
