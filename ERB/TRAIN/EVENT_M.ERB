TRYCALLFORM KOJO_EVENT_EXSTART_{NO:TARGET}_{CFLAG:209}, "タイトル"
ALIGNMENT LEFT
PRINTFORML 
DRAWLINE
;ループ开始

$EX_LOOP
NOWEX:暴发 = 0
CALL EVENT_EXPLAY
TRYCALLFORM KOJO_EVENT_EXSTART_{NO:TARGET}_{CFLAG:209}, "PLAY直后"
TRYCALLFORM KOJO_EVENT_EXSTART_{NO:TARGET}_{CFLAG:209}, "選択肢"

SIF GETBIT(TEQUIP:逆调教, 1)
	GOTO EX_END

;精力が无くなり、かつ斯道克が有るなら精力补充
IF BASE:PLAYER:精力 <= 0 && BASE:PLAYER:斯道克
	BASE:PLAYER:斯道克 -= 1
	BASE:PLAYER:精力 = MAXBASE:PLAYER:精力
	PRINTFORML %CALLNAME:PLAYER%的精力回復了！（回复完了！）
	PRINTFORML 
	SETCOLOR DEF_COLOR("黄色")
	PRINTFORML %CALLNAME:PLAYER%的精力残余 …… {BASE:PLAYER:精力}！
	PRINTFORMW 斯道克残余 …… {BASE:PLAYER:斯道克}！
	RESETCOLOR
	PRINTFORML 
ENDIF
TRYCALLFORM KOJO_EVENT_EXSTART_{NO:TARGET}_{CFLAG:209}, "PLAY一区切り"

SIF BASE:PLAYER:精力 > 0
	GOTO EX_LOOP

$EX_END
;斯道克が0（かつ精力が0以下）
IF BASE:PLAYER:精力 <= 0 && BASE:PLAYER:斯道克 == 0 && RAND:3
	;快乐刻印・淫乱を取得
	DRAWLINE
	PRINTFORML 
	TRYCALLFORM KOJO_KAIRAKU_{NO:TARGET}_{CFLAG:209}
	PRINTFORML 
	DRAWLINE
ENDIF


;-------------------------------------------------
;快乐调教関数
;任凭摆布から之逆调教で使用
;奉仕系メイン
;-------------------------------------------------
@EVENT_EXPLAY
PRINTFORML 
;调教メニュー一览
VARSET LOCALS
LOCAL = ABL:技巧 * 1000 + MAX(ABL:ＬＶ - ABL:PLAYER:ＬＶ, 0) * 1000 + ABL:ＬＶ * 500 + (MARK:PLAYER:快乐刻印 + TALENT:PLAYER:淫乱) * 1500

TRYCALLFORM SET_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}
LOCALS = %STR:100%

SELECTCASE LOCALS
CASE "指插れ", "按摩棒刺激", "自慰"
	LOCAL += ABL:指技 * 1000 + EXP:奉仕快乐経験 + RAND:10
	EXP:手交経験 += ABL:技巧/2 + 1
CASE "快乐强暴"
	LOCAL += ABL:腰技 * 1000 + EXP:奉仕快乐経験 + RAND:10
	EXP:Ｖ插入経験 += ABL:技巧/2 + 1
	EXP:PLAYER:Ｖ性交経験 += ABL:TARGET:技巧/2 + 1
	EXP:PLAYER:V经验 += ABL:TARGET:技巧/2 + 1
CASE "舐阴"
	LOCAL += ABL:舌技 * 1000 + EXP:奉仕快乐経験 + RAND:10
	EXP:舐阴経験 += ABL:技巧/2 + 1
CASE "口交"
	LOCAL += ABL:舌技 * 1000 + EXP:奉仕快乐経験 + RAND:100 + RAND:10
	EXP:口交経験 += ABL:技巧/2 + 1
CASE "乳交"
	LOCAL += (ABL:舌技 + ABL:魔乳) * 1000 +  EXP:奉仕快乐経験 + RAND:10
	EXP:乳交経験 += ABL:技巧/2 + 1
CASE "手交", "飞机杯奉侍"
	LOCAL += ABL:指技 * 1000 + EXP:奉仕快乐経験 + RAND:10
	EXP:手交経験 += ABL:技巧/2 + 1
CASE "接吻"
	LOCAL += ABL:舌技 * 1000 + EXP:奉仕快乐経験 + RAND:10
	EXP:接吻経験 += ABL:技巧/2 + 1
	EXP:PLAYER:接吻経験 += ABL:技巧/2 + 1
CASE "胸爱抚", "搾乳PLAY"
	LOCAL += (ABL:指技 + ABL:魔乳) * 1000 + EXP:奉仕快乐経験 + RAND:10
	EXP:手交経験 += ABL:技巧/2 + 1
CASE "尻尾阴茎刺激", "尻尾刺激"
	LOCAL += ABL:技巧 * 1000 + MIN(EXP:奉仕快乐経験 + CFLAG:2, MAXBASE:PLAYER:绝顶) + RAND:1000
	LOCAL *= 9
	EXP:嗜虐快乐経験 += ABL:技巧*9
ENDSELECT
;其他経験
IF TALENT:性别 == 0 && TALENT:PLAYER:性别 == 0
	EXP:百合経験 += RAND:5 + 1
	EXP:PLAYER:百合経験 += RAND:5 + 1
ELSEIF TALENT:性别== 1 && TALENT:PLAYER:性别== 1
	EXP:男同経験 += RAND:5 + 1
	EXP:PLAYER:男同経験 += RAND:5 + 1
ENDIF
EXP:嗜虐快乐経験 += ABL:技巧

TRYCALLFORM KOJO_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}, LOCALS

LOCAL = MULTIPLY(CALCF("基礎补正", LOCAL), CALCF("快乐补正", 100) )

CALL TEXT, "绝顶计算", LOCAL
;绝顶できなかった
IF CALCF("绝顶", PLAYER) == 0
	RETURN 0
;绝顶した
ELSE
	LOCALS:1 = 绝顶：%LOCALS%
	TRYCALLFORM KOJO_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}, LOCALS:1

	CALL BOUHATSU
	;追击判定
	;残精力20以下だと一定确率で追击、それ以上だとより低确率で追击。それとは别に斯道克でも判定
	IF (BASE:PLAYER:精力 <= 20 && RAND:2) || (BASE:PLAYER:精力 > 20 && RAND:4 == 0) || (BASE:PLAYER:斯道克 && RAND:5 == 0)
		IF BASE:PLAYER:斯道克 <= 0 && BASE:PLAYER:精力 <= 0
			PRINTFORMW %CALLNAME:PLAYER%已经到达极限了、%CALLNAME:TARGET%也没有把手放缓！
		ELSEIF BASE:PLAYER:精力 <= 0
			PRINTFORMW %CALLNAME:PLAYER%把精力毫无残存的全部向%CALLNAME:TARGET%献上了！
		ELSE
			PRINTFORMW %CALLNAME:PLAYER%的精力残余 …… {BASE:PLAYER:精力}！
		ENDIF
		PRINTFORML 

		LOCALS:1 = 追击：%LOCALS%
		TRYCALLFORM KOJO_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}, LOCALS:1

		;追击された场合は必殺
		LOCAL = 100000 + EXP:奉仕快乐経験 + RAND:10000
		CALL TEXT, "绝顶计算", LOCAL
		CALL BOUHATSU

		;余力がある场合はさらに追击判定(CALL BOUHATUなどはこ之台词之中で行う)
		IF BASE:PLAYER:精力 >= 10
			LOCALS:1 = ＥＸ追击：%LOCALS%
			TRYCALLFORM KOJO_EVENT_EXPLAY_{NO:TARGET}_{CFLAG:209}, LOCALS:1
		ENDIF

		IF BASE:PLAYER:精力 <= 0 && BASE:PLAYER:斯道克 == 0 
			IF PENIS(PLAYER) && RAND:4 == 0
				PRINTFORMW %CALLNAME:PLAYER%直到空空如也都一直被%CALLNAME:TARGET%吸吮了个干净！
			ELSEIF RAND:3 == 0
				PRINTFORMW %CALLNAME:PLAYER%精力一扫而尽的被%CALLNAME:TARGET%榨了个干净！
			ELSEIF RAND:2 == 0
				PRINTFORMW %CALLNAME:PLAYER%的精力全部被%CALLNAME:TARGET%美味地享用掉了！
			ELSE
				PRINTFORMW %CALLNAME:PLAYER%嘎啦嘎啦的像被绳缚住一样！
			ENDIF
		ELSEIF BASE:PLAYER:精力 <= 0
			PRINTFORMW %CALLNAME:PLAYER%把精力毫无残存的全部向%CALLNAME:TARGET%献上了！
		ELSE
			PRINTFORMW %CALLNAME:PLAYER%的精力残余 …… {BASE:PLAYER:精力}！
		ENDIF
		PRINTFORML 
	ENDIF
ENDIF
