@COM_ABLE178
;双人逆推指令实行判定
SIF COMABLE_DEF(178)
	RETURN 0
;MASTER调教中で无いとダメ
SIF ASSIPLAY
	RETURN 0
;气力が无いとダメ
SIF BASE:气力 == 0
	RETURN 0
;高潮管理中はダメ
SIF TEQUIP:高潮管理
	RETURN 0
;按摩棒插入中はダメ
SIF TEQUIP:按摩棒 || TEQUIP:肛用按摩棒
	RETURN 0
;跳蛋插入中はダメ
SIF TEQUIP:跳蛋插入
	RETURN 0
;演奏中はダメ
SIF TEQUIP:演奏中
	RETURN 0
;以下、助手之条件など
IF ASSI
	;妖精だとダメ（调教者が小根术ならＯＫ）
	;SIF TALENT:ASSI:妖精
	;	RETURN 0
	;恋慕、淫乱、誓約、相思相爱状态ならＯＫとする
	IF TALENT:ASSI:恋慕 || TALENT:ASSI:淫乱 || TALENT:ASSI:誓約 || TALENT:ASSI:相思相爱
	;除此以外なら、助手とTARGETが仲良くないとダメ。
	ELSE
		SIF RELATION:(NO:ASSI) < 120
			RETURN 0
	ENDIF
ENDIF
;实行值过滤

IF CONFIG("实行值过滤") || FLAG:10
	CALL COM_ORDER_COM178, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

