;-------------------------------------------------
;170～199番,～PLAY等舞台作成指令
;-------------------------------------------------
@COM_ABLE170
;录像摄影实行判定
SIF COMABLE_DEF(170)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:摄像机
	RETURN 1
;逆推之时には解除之み可能
SIF TEQUIP:逆推 && TEQUIP:摄像机 == 0
	RETURN 0
;一回に付き一本まで
SIF CSTR:81 != ""
	RETURN 0
;カメラとテープ之道具を持っているか、道具なし設定でないとダメ
SIF ( ITEM:摄像机 == 0 || ITEM:录像带 == 0 ) && NOITEM == 0
	RETURN 0
RETURN 1

@COM_ABLE171
;野外PLAY实行判定
SIF COMABLE_DEF(171)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:野外PLAY
	RETURN 1
;道具｢项圈｣を持っているか、道具なし設定、もしくは屈服刻印3LVか恋慕でないとダメ
SIF ITEM:项圈 == 0 && NOITEM == 0 && TALENT:恋慕 == 0 && MARK:屈服刻印 < 3
	RETURN 0
;顺从かM属性が3以上ないとダメ
SIF ABL:顺从 <= 2 && ABL:M属性 <= 2
	RETURN 0
;对象か助手が吸血鬼で、时间が昼だとダメ。夜之阳傘があるならＯＫ
IF TIME == 0 && ITEM:夜之阳傘 == 0
	SIF TALENT:吸血鬼 || (ASSI && TALENT:ASSI:吸血鬼)
		RETURN 0
ENDIF
;灌肠使用中は按摩棒がなければ不可
SIF TEQUIP:灌肠 && COND("肛塞") == 0
	RETURN 0
RETURN 1

@COM_ABLE172
;浴室PLAY实行判定
SIF COMABLE_DEF(172)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:浴室PLAY
	RETURN 1
;对象が兽耳で、顺从が3未满だとダメ
SIF TALENT:兽耳 && ABL:顺从 < 3
	RETURN 0
;羞恥PLAY中はダメ
SIF TEQUIP:羞恥PLAY
	RETURN 0
RETURN 1

@COM_ABLE173
;新婚PLAY实行判定
SIF COMABLE_DEF(173)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:新婚PLAY
	RETURN 1
;道具｢围裙｣を持っているか、道具なし設定でないとダメ
SIF ITEM:围裙 == 0 && NOITEM == 0
	RETURN 0
;助手じゃダメ
SIF ASSIPLAY
	RETURN 0
;顺从が3以上ないとダメ
SIF ABL:顺从 < 3
	RETURN 0
;羞恥PLAY中はダメ
SIF TEQUIP:羞恥PLAY
	RETURN 0
RETURN 1

@COM_ABLE174
;屋内露出PLAY
SIF COMABLE_DEF(174)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:屋内露出PLAY
	RETURN 1
;顺从かM属性が3以上ないとダメ
SIF ABL:顺从 <= 2 && ABL:M属性 <= 2
	RETURN 0
;羞恥PLAY中はダメ
SIF TEQUIP:羞恥PLAY
	RETURN 0
RETURN 1

@COM_ABLE175
;换装实行判定
SIF COMABLE_DEF(175)
	RETURN 0
;恍惚状态だとダメ
SIF STATE("恍惚", PLAYER)
	RETURN 0
IF TEQUIP:逆推
	SIF CHECK_CLO("裸露")
		RETURN 0
;绳使用中は脱光こと之み可能

ELSEIF TEQUIP:拘束
	SIF CHECK_CLO("裸露")
		RETURN 0
ENDIF
;野外PLAYor屋内露出PLAY中は、露阴癖3Lvか顺从4Lvが无いとダメ
SIF (TEQUIP:屋内露出PLAY || TEQUIP:野外PLAY) && ABL:顺从 < 4 && ABL:露阴癖 < 3
	RETURN 0
;实行值过滤

IF CONFIG("实行值过滤") || FLAG:10
	CALL COM_ORDER_COM175, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

@COM_ABLE176
;逆推指令实行判定
SIF COMABLE_DEF(176)
	RETURN 0
;气力が无いと侍奉精神がないとダメ
SIF BASE:气力 == 0 && ABL:侍奉精神 < 3
	RETURN 0
;高潮管理中はダメ
SIF TEQUIP:高潮管理
	RETURN 0
;按摩棒插入中はダメ
SIF TEQUIP:按摩棒 || TEQUIP:肛用按摩棒
	RETURN 0
;跳蛋插入中はダメ
SIF TEQUIP:跳蛋插入
	RETURN 0
IF CONFIG("实行值过滤") || FLAG:10
	CALL COM_ORDER_COM176, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

