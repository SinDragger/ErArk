@COM_ABLE92
;乳交实行判定
SIF COMABLE_DEF(92)
	RETURN 0
;胸を剥き出しにできるor胸露出でないとダメ
SIF CHECK_CLO("谷间露出") + CHECK_CLO("可裸露") == 0
	RETURN 0
;性别男だとダメ
SIF TALENT:性别 == 1
	RETURN 0
;[巨乳]でなければ技巧2以上、[贫乳]だと技巧3以上必要
SIF (TALENT:贫乳 && ABL:技巧 < 3) || (TALENT:巨乳 == 0 && TALENT:贫乳 == 0 && ABL:技巧 < 2)
	RETURN 0
;调教者が性别男か扶她じゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0
;实行值过滤

IF CONFIG("实行值过滤") || FLAG:10
	CALL COM_ORDER_COM92, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

