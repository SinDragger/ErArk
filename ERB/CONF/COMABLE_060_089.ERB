;80番台,特殊性爱系指令
;-------------------------------------------------
@COM_ABLE80
;磨镜实行判定
SIF COMABLE_DEF(80)
	RETURN 0
;V剥き出しにできないとダメ
SIF !CHECK_CLO("V露出可")
	RETURN 0
;性别男だとダメ
SIF TALENT:性别 == 1 || TALENT:PLAYER:性别 == 1
	RETURN 0
;双头按摩棒以外之按摩棒插入中はダメ(PLAYERが双头按摩棒で处女丧失する之はどうなんだろ)
IF TEQUIP:按摩棒
	SIF COND("双头按摩棒") == 0
		RETURN 0
	IF TALENT:PLAYER:处女
		IF TEQUIP:逆推
			SIF ABL:S属性 < 3
				RETURN 0
		ELSE
			SIF ABL:PLAYER:M属性 < 3
				RETURN 0
		ENDIF
	ENDIF
ENDIF
;阴蒂夹使用中は不可
SIF TEQUIP:阴蒂夹
	RETURN 0
RETURN 1

