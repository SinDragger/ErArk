	RETURN 0
RETURN 1

@COM_ABLE3
;自慰实行判定
SIF COMABLE_DEF(3)
	RETURN 0
;绳・拘束中で手が使えない时はダメ
SIF GETBIT(TEQUIP:拘束, 20) || STATE("束缚术：手")
	RETURN 0
;实行值过滤
IF CONFIG("实行值过滤") || FLAG:10
	CALL COM_ORDER_COM3, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

