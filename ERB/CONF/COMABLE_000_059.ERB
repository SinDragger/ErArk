@COM_ABLE6
;接吻实行判定
SIF COMABLE_DEF(6)
	RETURN 0
;爱之圈套に掛かっているとダメ恋慕ならヨシ！
SIF TEQUIP:爱之圈套 && TALENT:恋慕 == 0
	RETURN 0
;逆推ているとダメ…接吻经验が丰富（接吻好き）or恋慕なら良し！
SIF TEQUIP:逆推 && TALENT:恋慕 == 0 && EXP:接吻经验 < 20
	RETURN 0
;球形口枷使用中は不可
SIF TEQUIP:口枷
	RETURN 0
;实行值过滤
IF CONFIG("实行值过滤") || FLAG:10
	CALL COM_ORDER_COM6, 1
	RETURN LIMIT(RESULT, 0, 1)
ENDIF
RETURN 1

