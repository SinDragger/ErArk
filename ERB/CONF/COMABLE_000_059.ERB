@COM_ABLE0
;爱抚实行判定
SIF COMABLE_DEF(0)
	RETURN 0
SIF COND("指之使用", PLAYER) == 0
	RETURN 0
RETURN 1

@COM_ABLE1
;舐阴实行判定
SIF COMABLE_DEF(1)
	RETURN 0
SIF TALENT:性别 == 1
	RETURN 0
IF TEQUIP:逆推
	SIF TEQUIP:按摩棒 || TEQUIP:肛用按摩棒
		RETURN 0
ELSE
	SIF CHECK_OUTER("裤") && !CHECK_CLO("V露出可")
		RETURN 0
ENDIF
RETURN 1

