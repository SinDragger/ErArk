@ATTACK_13,ARG:0,ARG:1,ARGS   ;3*4远攻模板
#DIM COUNT3  ;DO NOT USE COUNT AND COUNT2
#DIM COUNT4
#DIM COUNT5
LOCAL=FINDCHARA(NO,13)
FOR COUNT3,0,4
    FOR COUNT4,-1,2
        IF ARG:0+COUNT4>=0&&ARG:0+COUNT4<=20
        IF ENEMY:(ARG:0+COUNT4):(ARG:1+COUNT3):0!=0
            FOR COUNT5,0,20
                IF ENEMY:(ARG:0+COUNT4):(ARG:1+COUNT3):COUNT5!=0
                    CALL DAMAGE_物理(BASE:LOCAL:攻击,0,ENEMY:(ARG:0+COUNT4):(ARG:1+COUNT3):COUNT5,LOCAL)
                    ENEMYH:(ARG:0+COUNT4):(ARG:1+COUNT3):COUNT5-=RESULT
                    PRINTFORMW 空爆对%CSTRNAME:(ENEMY:(ARG:0+COUNT4):(ARG:1+COUNT3):COUNT5)%造成{RESULT}法术伤害
                ENDIF
            NEXT
        ENDIF
        ENDIF
    NEXT
NEXT
RETURN

