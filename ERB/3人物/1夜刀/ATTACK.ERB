@ATTACK_1,ARG:0,ARG:1,ARGS   ;两格先锋模板
LOCAL=FINDCHARA(NO,1)
IF ENEMY:(ARG:0):(ARG:1):0!=0
    CALL DAMAGE_物理(BASE:LOCAL:5,0,ENEMY:(ARG:0):(ARG:1):0,1)
    ENEMYH:(ARG:0):(ARG:1):0-=RESULT
    PRINTFORML 夜刀对%CSTRNAME:(ENEMY:(ARG:0):(ARG:1):0)%造成{RESULT}物理伤害
    PRINTW
ELSEIF ENEMY:(ARG:0):(ARG:1+1):0!=0
    CALL DAMAGE_物理(BASE:LOCAL:5,0,ENEMY:(ARG:0):(ARG:1+1):0,1)
    ENEMYH:(ARG:0):(ARG:1+1):0-=RESULT
    PRINTFORML 夜刀对%CSTRNAME:(ENEMY:(ARG:0):(ARG:1+1):0)%造成{RESULT}物理伤害
    PRINTW
ENDIF
RETURN

@SKILL_1,ARG:0,ARG:1,ARG:2 ;坐标YX，是否是手动激活及撤销技能
