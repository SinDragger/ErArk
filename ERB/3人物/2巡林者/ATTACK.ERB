@ATTACK_2,ARG:0,ARG:1,ARGS   ;3*4远攻模板
#DIM COUNT3  ;DO NOT USE COUNT AND COUNT2
#DIM COUNT4
LOCAL=FINDCHARA(NO,2)
FOR COUNT3,0,4
    FOR COUNT4,-1,2
        IF ARG:0+COUNT4>=0&&ARG:0+COUNT4<=20
        IF ENEMY:(ARG:0+COUNT4):(ARG:1+COUNT3):0!=0
            CALL DAMAGE_物理(BASE:LOCAL:攻击,0,ENEMY:(ARG:0+COUNT4):(ARG:1+COUNT3):0,LOCAL)
            ENEMYH:(ARG:0+COUNT4):(ARG:1+COUNT3):0-=RESULT
            PRINTFORML 巡林者对%ENEMY_NAME:(ENEMY:(ARG:0+COUNT4):(ARG:1+COUNT3):0)%造成{RESULT}物理伤害
            PRINTW
            RETURN
        ENDIF
        ENDIF
    NEXT
NEXT
RETURN

