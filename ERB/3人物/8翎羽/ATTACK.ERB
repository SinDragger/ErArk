@ATTACK_8,ARG:0,ARG:1,ARGS   ;两格先锋模板
LOCAL=FINDCHARA(NO,8)
IF ENEMY:(ARG:0):(ARG:1):0!=0
    CALL DAMAGE_物理(BASE:LOCAL:攻击,0,ENEMY:(ARG:0):(ARG:1):0,1)
    ENEMYH:(ARG:0):(ARG:1):0-=RESULT
    SIF ENEMYH:(ARG:0):(ARG:1):0<0
        COST += 2
    PRINTFORML 翎羽对%CSTRNAME:(ENEMY:(ARG:0):(ARG:1):0)%造成{RESULT}物理伤害\@ ENEMYH:(ARG:0):(ARG:1):0<0 ?并回复2点COST# \@
    PRINTW
ELSEIF ENEMY:(ARG:0):(ARG:1+1):0!=0
    CALL DAMAGE_物理(BASE:LOCAL:攻击,0,ENEMY:(ARG:0):(ARG:1+1):0,1)
    ENEMYH:(ARG:0):(ARG:1+1):0-=RESULT
    SIF ENEMYH:(ARG:0):(ARG:1+1):0<0
        COST += 2
    PRINTFORML 翎羽对%CSTRNAME:(ENEMY:(ARG:0):(ARG:1+1):0)%造成{RESULT}物理伤害\@ ENEMYH:(ARG:0):(ARG:1+1):0<0 ?并回复2点COST# \@
    PRINTW
ENDIF
RETURN

