@ATTACK_9,ARG:0,ARG:1,ARGS   ;两格先锋模板
LOCAL=FINDCHARA(NO,9)
IF ENEMY:(ARG:0):(ARG:1):0!=0
    CALL DAMAGE_物理(BASE:LOCAL:攻击,0,ENEMY:(ARG:0):(ARG:1):0,LOCAL)
    ENEMYH:(ARG:0):(ARG:1):0-=RESULT
    PRINTFORML 芬对%CSTRNAME:(ENEMY:(ARG:0):(ARG:1):0)%造成{RESULT}物理伤害
    PRINTW
ELSEIF ENEMY:(ARG:0):(ARG:1+1):0!=0
    CALL DAMAGE_物理(BASE:LOCAL:攻击,0,ENEMY:(ARG:0):(ARG:1+1):0,LOCAL)
    ENEMYH:(ARG:0):(ARG:1+1):0-=RESULT
    PRINTFORML 芬对%CSTRNAME:(ENEMY:(ARG:0):(ARG:1+1):0)%造成{RESULT}物理伤害
    PRINTW
ENDIF
RETURN

@SKILL_9,ARG:0,ARG:1,ARG:2 ;坐标YX，是否是手动激活及撤销技能
LOCAL=FINDCHARA(NO,9)
SIF ARG:2==1
    PRINTW 没有主动技能
IF ARG:2==0
    IF BASE:LOCAL:技力 >= 5
        PRINTW  芬的冲锋号令恢复了12COST
        COST += 12
        BASE:LOCAL:技力 = 0
    ENDIF
ENDIF
RETURN -1