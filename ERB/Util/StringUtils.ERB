@GET_STRING_COLOR, ARGS
#FUNCTION
SELECTCASE ARGS
	CASE "深红色"
		RETURNF 0xF06050
	CASE "红色"
		RETURNF 0xC07070
	CASE "青色"
		RETURNF 0x7070C0
	CASE "淡蓝色";基建技能颜色
		RETURNF 0x87CEFA
	CASE "绀色"
		RETURNF 0x6666FF
	CASE "绿色"
		RETURNF 0x66DD66
	CASE "紫色"
		RETURNF 0xC070C0
	CASE "黄色"
		RETURNF 0xC0B050
	CASE "青绿色"
		RETURNF 0x70C0C0
	CASE "灰色"
		RETURNF 0x666666
	 CASEELSE
		RETURNF 0xC0C0C0
ENDSELECT